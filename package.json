{
  "name": "servie-send",
  "version": "0.0.1",
  "description": "Generate a HTTP response with client-side cache support",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "lint": "tslint \"src/**/*.ts\" --project tsconfig.json --type-check",
    "build": "rm -rf dist/ && tsc",
    "specs": "jest --coverage",
    "test": "npm run -s lint && npm run -s build && npm run -s specs",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/serviejs/servie-send.git"
  },
  "keywords": [
    "servie",
    "send",
    "etag",
    "cache",
    "http",
    "body"
  ],
  "author": {
    "name": "Blake Embrey",
    "email": "hello@blakeembrey.com",
    "url": "http://blakeembrey.me"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/serviejs/servie-send/issues"
  },
  "homepage": "https://github.com/serviejs/servie-send",
  "devDependencies": {
    "@types/jest": "^19.2.3",
    "@types/node": "^7.0.18",
    "jest": "^20.0.0",
    "rimraf": "^2.5.4",
    "servie": "^0.2.5",
    "throwback": "^2.0.0",
    "ts-jest": "^20.0.2",
    "tslint": "^5.0.0",
    "tslint-config-standard": "^6.0.1",
    "typescript": "^2.2.1"
  },
  "peerDependencies": {
    "servie": "^0.2.0"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest/preprocessor.js"
    },
    "testRegex": "src/.*\\.(?:test|spec)\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
